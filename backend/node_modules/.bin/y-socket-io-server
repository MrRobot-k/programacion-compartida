#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/dist/server/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/dist/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/dist/server/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/dist/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules:/home/esdras/Escritorio/editor-colaborativo/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/dist/server/server.js" "$@"
else
  exec node  "$basedir/../.pnpm/y-socket.io@1.1.3_yjs@13.6.27/node_modules/y-socket.io/dist/server/server.js" "$@"
fi
